type Category{
    id: ID!,
    name: String!
}

input CategoryInput {
    id: ID,
    name: String!
}

extend type Query {
  findCategoryById(id:ID!):Category
  findAllCategory:[Category]
}

extend  type Mutation {
    deleteCategoryById(id:ID!):Boolean
    saveCategory(category:CategoryInput!): Category
}
